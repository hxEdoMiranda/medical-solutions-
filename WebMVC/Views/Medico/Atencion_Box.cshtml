    <script type="text/javascript">
        window.initInterval = (milliseconds) => {
            const cronn = document.querySelector(".containerCronn .cronn .val");
            const millisA = milliseconds;
            cronn.textContent = `00:00:00`;
            let interval = null;
            //interrumpir interval
            clearInterval(window.initInterval);
            return {
                init: () => {
                    interval = setInterval(() => {
                        const millisB = Date.now();
                        const a = new Date(millisB - millisA);
                        cronn.textContent = `${("" + a.getUTCHours()).padStart(2, '0')}:${('' + a.getMinutes()).padStart(2, '0')}:${('' + a.getSeconds()).padStart(2, '0')}`;
                    }, 1000)
                },
                clearInterval: () => {
                    clearInterval(interval)
                }
            };
        };
        let cronn = window.initInterval(Date.now()).init();
    </script>

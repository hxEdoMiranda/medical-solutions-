@model WebMVC.Models.AtencionViewModel

@{
    ViewBag.Title = "Medical | Sitio Médicos";
    Layout = "_Layout";
    ViewData["view"] = Roles.Medico;

    var atentionId = Model.Atencion.HoraMedico.IdEspecialidad;
    var tituloInforme = atentionId.ToString() == "129" ? "Informe de enfermería" : "Informe Médico";
    var hiddenUnab = "";
    var hiddenUOHBoxEspecial = "";
    var isPsicopedagogiaUnab = false;
    var hiddenPsicopedagogiaUnab = "";
    var tituloControl = "Control";
    var tituloIndicaciones = "Indicaciones";
    var tituloCertificadoMedico = "Certificado Médico";
    var psicologia = false;
    var isPositivaPsicologiaColombia = false;
    var hiddenPositivaPsicologia = "";
    var idClientePositiva = 722;
    
       @if (ViewBag.HostURL.ToString().Contains("localhost") || ViewBag.HostURL.ToString().Contains("qa.")){
                 idClientePositiva = 443;
           }
    if (ViewBag.HostURL.ToString().Contains("uoh."))
    {
        tituloInforme = "Reporte de Salud";
        hiddenUOHBoxEspecial = ViewBag.boxEspecial == 1 ? "hidden" : string.Empty;
    }

    if (ViewBag.boxEspecial == 1)
    {
        tituloIndicaciones = "Plan";
    }
    else if (ViewBag.HostURL.ToString().Contains("unabactiva.") || ViewBag.HostURL.ToString().Contains("activa.unab."))
    {
        tituloIndicaciones = isPsicopedagogiaUnab ? "Acuerdos PSP" : "Plan de manejo";
    }

    var tituloForm = "Reporte Médico";
    var tituloAnamesis = "Anamnesis/Hipótesis Diagnóstica";
    var tituloDg = "Diagnóstico";
    var hiddenPsico = "hidden";
    var hiddenPNutri = "hidden";
    var tituloOtrosEx = "Otros Exámenes";
    var hiddenPsicoInput = "";
    var hiddenNutriInput = "";

    if (ViewBag.HostURL.ToString().Contains("unabactiva.") || ViewBag.HostURL.ToString().Contains("activa.unab."))
    {
        tituloInforme = "Reporte atención de salud";
        hiddenUnab = "hidden";
        tituloForm = "Reporte de salud";
        tituloAnamesis = "Anamnesis / Antecedentes";
        tituloIndicaciones = "Plan de manejo";

        // Especialidad
        string especialidad = Model.Atencion.HoraMedico?.Especialidad ?? Model.Atencion.Especialidad ?? string.Empty;
        isPsicopedagogiaUnab = Util.RemoveAccent(especialidad).ToLower().Contains("psicopedagogia");

        if (isPsicopedagogiaUnab)
        {
            tituloForm = "Reporte de Psicopedagogía";
            tituloAnamesis = "Anamnesis / Antecedentes Psicopedagógicos";
            tituloIndicaciones = "Acuerdos PSP";
            tituloCertificadoMedico = "Sugerencias, derivaciones";
            tituloControl = "Próxima sesión PSP";
            hiddenPsicopedagogiaUnab = "hidden";
        }
    }

    if (Model.Atencion.HoraMedico.IdEspecialidad == 47) // psicologia
    {
        tituloForm = "Reporte de salud";
        tituloAnamesis = "Anamnesis/ Antecedentes área Psicología";
        tituloDg = "Diagnostico C10 con Snomed";
        hiddenPsico = "";
        tituloOtrosEx = "Diagnóstico área psicológica";
        tituloIndicaciones = "Indicaciones/Plan";
        hiddenPsicoInput = "hidden";
    }
    else if (Model.Atencion.HoraMedico.IdEspecialidad == 54) // nutricion y dietetica
    {
        tituloForm = "Reporte de salud";
        tituloAnamesis = "Anamnesis/ Antecedentes nutricionales - Hábitos";
        tituloDg = "Diagnostico C10 con Snomed";
        hiddenPNutri = "";
        tituloOtrosEx = "Diagnóstico área nutricional";
        tituloIndicaciones = "Plan/Indicaciones";
        hiddenNutriInput = "hidden";

    }
    else if (Model.Atencion.HoraMedico.IdEspecialidad == 4) // pediatria
    {
        tituloAnamesis = "Anamnesis/ Antecedentes médicos/ Antecedentes familiares";
        tituloDg = "Diagnostico C10 con Snomed";
        tituloIndicaciones = "Indicaciones no farmacológicas";
    }

    if ((Model.Atencion.HoraMedico?.IdEspecialidad ?? Model.Atencion.IdEspecialidad) == 47) // psicologia
    {
        psicologia = true;


    }
    else if ((Model.Atencion.HoraMedico?.IdEspecialidad ?? Model.Atencion.IdEspecialidad) == 91) // psicologia
    {
        psicologia = true;
    }

    if (psicologia && Model.Atencion.IdCliente == idClientePositiva && Model.Atencion.CodigoPais == "CO"){
        hiddenPositivaPsicologia = "hidden";
        isPositivaPsicologiaColombia = true;
        tituloControl = "Conclusiones y comentarios";
    }
    

}

@{
    Layout = "_LayoutPaciente";
}

@if(ViewBag.HostUrl.Contains("clinicamundoscotia.")){
    <partial name="~/Views/FarmaciaOnline/FarmaciaClinicaMundo.cshtml" model="Model" view-data="ViewData" />
}
else if (ViewBag.HostUrl.Contains("masproteccionsalud.")){
    <partial name="~/Views/FarmaciaOnline/FarmaciaClinicaCaja.cshtml" model="Model" view-data="ViewData" />
}



@section Scripts {
    <script type="module">
        import { init } from '../../js/Paciente/loguso.js?rnd=${@NumeroRandom.GetRandom()}';
        init(@Html.Raw(Json.Serialize(Model)))
    </script>


}
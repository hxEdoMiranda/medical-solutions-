                                    @* <div class="form-group">
                                    <label id="sigvitales_enfermera_datos" style="font-weight:bold">Signos Vitales</label>
                                    <input class="form-control" rows="3" name="sigvitales_enfermera_datos" id="sigvitales_enfermera_datos_id" />
                                    </div>
                                    <div class="form-group">
                                    <label id="Prearterial_enfermera_datos" style="font-weight:bold">Presión Arterial</label>
                                    <input class="form-control" rows="3" name="Prearterial_enfermera_datos" id="Prearterial_enfermera_datos_id" />
                                    </div>
                                    <div class="form-group">
                                    <label id="glicemia_enfermera_datos" style="font-weight:bold">Glicemia</label>
                                    <input class="form-control" rows="3" name="glicemia_enfermera_datos" id="glicemia_enfermera_datos_id" />
                                    </div>*@
                                </div>

                                <!--Habitos -->
                                <div>
                                    <hr style="width: 100%;">
                                    <h5>Hábitos</h5>

                                    <div class="form-group d-flex">
                                        <label id="tabaco_enfermera_datos" style="font-weight:bold">Tabaco</label>
                                        <div class="form-check form-check-inline">
                                            <input class="form-check-input ml-2" type="radio" name="tabaco_enfermera_datos" value="si" id="tabaco_si">
